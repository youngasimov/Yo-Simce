<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.panel {
			display:block;
			width: 300px;
			margin-left:auto;
			margin-right: auto;
			margin-top: 200px; 
		}
		.button {
			display:block;
			width:80%;
			margin-left:10%;
			float: left;
			margin-bottom: 3em;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames='{style.panel}'>
		<g:Button addStyleNames="{style.button}" ui:field="ingresoMaterialActionButton" text="Ingreso de material"/>
		<g:Button addStyleNames="{style.button}" ui:field="salidaMaterialActionButton" text="Salida de material"/>
		<g:Button addStyleNames="{style.button}" ui:field="movimientoMaterialViewButton" text="Movimientos de material"/>
	</g:HTMLPanel>
</ui:UiBinder> 