<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.scroll{
			width: 350px;
		}
		
		.list{
			width: 320px;
			margin-left: auto;
			margin-right: auto;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:ScrollPanel addStyleNames='{style.scroll}' ui:field="scroll">
			<c:CellList ui:field="coList" addStyleNames='{style.list}'/> 
		</g:ScrollPanel>
	</g:HTMLPanel>
</ui:UiBinder> 