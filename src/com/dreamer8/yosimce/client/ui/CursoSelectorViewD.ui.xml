<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.dialogbox {
			width: 400px;
		}
		.panel {
			display:block;
			float: left;
		}
	</ui:style>
	<g:DialogBox animationEnabled="true" autoHideEnabled="true" autoHideOnHistoryEventsEnabled="true" glassEnabled="true" modal="true"  ui:field="panel">
		<g:HTMLPanel>
			<table>
				<tr>
					<td colspan='2'>Buscar establecimiento</td>
				</tr>
				<tr>
					<td><g:SuggestBox width="80px" ui:field="rbdBox"/></td>
					<td> <g:Button ui:field="searchButton" text="Buscar"/> </td>
				</tr>
				<tr>
					<td colspan='2'> <c:CellList ui:field="cursosList"/> </td>
				</tr>
				<tr>
					<td><g:Button ui:field="cancelBox" text="Cancelar"/></td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:DialogBox>
</ui:UiBinder> 