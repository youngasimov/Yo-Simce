<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

		.content{
			display:block;
			width: 400px;
			margin-left: auto;
			margin-right: auto;
			margin-top: 300px;
		}
		.button {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
	    <g:FlowPanel styleName='{style.content}'>
			<g:Button addStyleNames="{style.button}" ui:field="planAndResultButton" />
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 