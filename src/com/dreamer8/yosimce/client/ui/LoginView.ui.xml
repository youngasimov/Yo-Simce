<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.panel{
			display:block;
			width: 400px;
			margin-left: auto;
			margin-right: auto;
			margin-top: 200px; 
		}
	</ui:style>
	<g:HTMLPanel>
	    
	    <g:DecoratorPanel addStyleNames='{style.panel}'>
			<g:HTMLPanel>
				<table>
					<tr>
						<td>Usuario:</td>
						<td> <g:TextBox ui:field="usernameBox"/> </td>
					</tr>
					<tr>
						<td></td>
						<td> <g:Button ui:field="loginButton" text="Ingresar"/> </td>
					</tr>
				</table>
			</g:HTMLPanel>    	
	    </g:DecoratorPanel>
	    
	</g:HTMLPanel>
</ui:UiBinder> 