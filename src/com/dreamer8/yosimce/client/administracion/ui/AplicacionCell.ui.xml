<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='nombre' type='java.lang.String'/>
    
	<ui:style>
		.nombre{
			display:block;
			width: 95%;
			padding-top: 1em;
			padding-bottom: 1em;
			padding-left: 5%;
			font-size: 15px;
			font-weight: bold;
		}
	</ui:style>
	<div class='{style.nombre}'>
	    <ui:text from='{nombre}' />
	</div>
</ui:UiBinder> 