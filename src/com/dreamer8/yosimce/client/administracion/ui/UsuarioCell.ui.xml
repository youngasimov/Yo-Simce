<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
    
    <ui:with field='nombre' type='java.lang.String'/>
    
	<ui:style>
		.nombre{
			display:block;
			float: left;
			width: 100%;
			height: 3em;
			font-size: 13px;
			font-weight: bold;
		}
	</ui:style>
	<div class='style.nombre'>
	    <ui:text from='{nombre}' />
	</div>
</ui:UiBinder>