<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.header{
		display:block;
		width: 100%;
		height: 50px;
		float: left;
		position: absolute;
		top: 0px;
		left: 0px;
		padding-left: 80px;
		padding-top: 10px;
		border-bottom: 1px solid black;
	}
	
	.content{
		display:block;
		float: left;
		width: 100%;
		height: 100%;
		position:absolute;
		top: 50px;
		bottom: 0px;
		left: 0px;
	}
	
	.title {
		display:inline-block;
		position: relative;
		height: 50px;
		float: left;
		font-size: 26px;
	}
	
	</ui:style>
	<g:DockLayoutPanel unit="EM">
		<g:north size="4">
			<g:HTMLPanel addStyleNames='{style.header}'>
				<div class='{style.title}'>Administraci√≥n de usuarios </div>
			</g:HTMLPanel>	
		</g:north>
		<g:west size="18">
			<g:HTMLPanel>
				<div>
					<g:TextBox ui:field="searchBox" width="11em"/>
					<g:Button ui:field="cleanButton" width="6em" text="limpiar"/>
				</div>
				<div>
					<g:ScrollPanel>
						<c:CellList ui:field="usuariosList"/>
					</g:ScrollPanel>
				</div>
			</g:HTMLPanel>
		</g:west>
		<g:center>
			<g:HTMLPanel>
			</g:HTMLPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 