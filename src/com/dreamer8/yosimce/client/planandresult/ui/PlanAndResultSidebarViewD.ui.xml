<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cell="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.stack {
			font-size: 10pt;
		}
		.list {
			display:block;
			width: 100%;
			height: 98%;
			float: left;
			border-bottom: 1px solid black;
			border-top: 1px solid black;
		}
	</ui:style>
	<g:StackLayoutPanel addStyleNames='{style.stack}' unit='EM' ui:field="stack">
		<g:stack>
			<g:header size='3'>Filtros</g:header>
			
			<g:Label>able</g:Label>
			
		</g:stack>
		<g:stack>
			<g:header size='3'>Establecimientos</g:header>
			
			<g:DockLayoutPanel unit='EM'>
				<g:north size='1.5'>
					<g:CheckBox text='Seleccionar todo' ui:field="selectAllBox"/>
				</g:north>
				<g:south size='1.5'>
					<g:Label>pager</g:Label>
				</g:south>
				<g:center>
					<g:FlowPanel styleName='{style.list}'>
						<cell:CellList ui:field="establecimientosCellList"/>
					</g:FlowPanel>
				</g:center>
			</g:DockLayoutPanel>
			
		</g:stack>
	</g:StackLayoutPanel>
</ui:UiBinder> 