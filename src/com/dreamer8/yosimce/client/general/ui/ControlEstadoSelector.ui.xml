<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<table>
		    <tr>
		        <td> Sin Movimiento </td>
		        <td> <img href='/images/wait.png' width='64px' hight='64px' /> </td>
		        <td> <g:Button ui:field="waitButton" text="Seleccionar"/> </td>
		    </tr>
		    <tr>
		        <td> Todo Salió </td>
		        <td> <img href='/images/round_trip.png' width='64px' hight='64px' /> </td>
		        <td> <g:Button ui:field="idaButton" text="Seleccionar"/> </td>
		    </tr>
		    <tr>
		        <td> Todo Volvió </td>
		        <td> <img href='/images/round_trip2.png' width='64px' hight='64px' /> </td>
		        <td> <g:Button ui:field="vueltaButton" text="Seleccionar"/> </td>
		    </tr>
		    <tr>
		        <td> Con problemas </td>
		        <td> <img href='/images/big_warning.png' width='64px' hight='64px' /> </td>
		        <td> <g:Button ui:field="problemaButton" text="Seleccionar"/> </td>
		    </tr>
		    
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 