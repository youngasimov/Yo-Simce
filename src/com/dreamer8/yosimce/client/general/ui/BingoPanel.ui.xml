<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.bingo{
		display:block;
		width: 100%;
		height: 100%;
	}
	
	.bingo td{
		font-size: 13px;
		font-weight: bold;
	}
	
	.rm{
		display:block;
		width: 45%;
		height: 100%;
		float: left;
		border-right: 2px solid black;
		padding:2%;
	}
	
	.r8{
		display:block;
		width: 45%;
		height: 100%;
		float: right;
		border-left: 2px solid black;
		padding:2%;
	}
	</ui:style>
	<g:HTMLPanel>
	    <div class='{style.bingo}'>
			<div class='{style.rm}'>
				<g:FlexTable ui:field="bingormTable" width="100%" height="100%"/>
			</div>
			<div class='{style.r8}'>
				<g:FlexTable ui:field="bingor8Table" width="100%" height="100%"/>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 