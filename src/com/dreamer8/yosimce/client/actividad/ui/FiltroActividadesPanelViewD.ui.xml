<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.panel .title td{
			font-weight: bold;
			text-align: center;
			border-top: 2px solid black;
			border-bottom: 1px solid #AAAAAA;
		}
	</ui:style>
	<g:HTMLPanel>
		<table class='{style.panel}'>
			<tr class='{style.title}'>
				<td colspan='2'>Estado</td>
			</tr>
			<tr>
				<td colspan='2'> <g:VerticalPanel ui:field="estadosPanel"/> </td>
			</tr>
			<tr class='{style.title}'>
				<td colspan='2'>Opciones</td>
			</tr>
			<tr>
				<td>Usó material contingencia:</td>
				<td><g:CheckBox ui:field="contingenciaBox"/> </td>
			</tr>
			<!--
			<tr>
				<td>Contingencia momentanea:</td>
				<td><g:CheckBox ui:field="problemasBox"/> </td>
			</tr>
			<tr>
				<td>Contingencia inhabilitante:</td>
				<td><g:CheckBox ui:field="problemasHinabilitantesBox"/> </td>
			</tr>
			 -->
			<tr>
				<td>No sincronizadas:</td>
				<td><g:CheckBox ui:field="sincronizadasNulaBox"/> </td>
			</tr>
			<tr>
				<td>Parcialmente sincronizadas:</td>
				<td><g:CheckBox ui:field="sincronizacionParcialBox"/> </td>
			</tr>
			<tr>
				<td>Totalmente sincronizadas:</td>
				<td><g:CheckBox ui:field="sincronizacionTotalBox"/> </td>
			</tr>
			
			<tr class='{style.title}'>
				<td colspan='2'>Lugar</td>
			</tr>
			<tr>
				<td>Región:</td>
				<td><g:ListBox ui:field="regionBox"/></td>
			</tr>
			<tr>
				<td>Comuna:</td>
				<td><g:ListBox ui:field="comunaBox"/></td>
			</tr>
			<tr class='{style.title}'>
				<td><g:Button ui:field="cancelarButton" text="Cancelar"/></td>
				<td><g:Button ui:field="aplicarButton" text="Aplicar"/> </td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 